# ============================================
# Database Configuration
# ============================================
# MySQL/TiDB connection string
# Format: mysql://user:password@host:port/database
DATABASE_URL=mysql://user:password@localhost:3306/carfax_db

# ============================================
# Apify Configuration
# ============================================
# Your Apify Actor ID for Carfax scraping
APIFY_ACTOR_ID=your_actor_id_here

# Your Apify API Key (get from https://console.apify.com/account/integrations)
APIFY_API_KEY=your_apify_api_key_here

# ============================================
# Carfax Credentials
# ============================================
# Your Carfax account email
CARFAX_EMAIL=your_carfax_email@example.com

# Your Carfax account password
CARFAX_PASSWORD=your_carfax_password_here

# ============================================
# Authentication & Security
# ============================================
# JWT secret key for session signing (generate a random string)
JWT_SECRET=your_jwt_secret_key_here_min_32_chars

# Manus OAuth server URL
OAUTH_SERVER_URL=https://api.manus.im

# ============================================
# Manus Configuration
# ============================================
# Your Manus application ID
VITE_APP_ID=your_manus_app_id

# Manus OAuth portal URL
VITE_OAUTH_PORTAL_URL=https://portal.manus.im

# Owner information
OWNER_NAME=Your Name
OWNER_OPEN_ID=your_open_id

# ============================================
# Manus Built-in APIs
# ============================================
# Manus API URL for LLM, storage, notifications, etc.
BUILT_IN_FORGE_API_URL=https://api.manus.im

# Bearer token for backend access to Manus APIs
BUILT_IN_FORGE_API_KEY=your_forge_api_key_here

# Frontend access token for Manus APIs
VITE_FRONTEND_FORGE_API_KEY=your_frontend_forge_api_key_here

# Frontend URL for Manus APIs
VITE_FRONTEND_FORGE_API_URL=https://api.manus.im

# ============================================
# Analytics (Optional)
# ============================================
# Analytics endpoint for tracking
VITE_ANALYTICS_ENDPOINT=https://analytics.example.com

# Analytics website ID
VITE_ANALYTICS_WEBSITE_ID=your_analytics_id

# ============================================
# Application Configuration
# ============================================
# Application title (displayed in browser tab)
VITE_APP_TITLE=Carfax Manus

# Application logo URL
VITE_APP_LOGO=https://example.com/logo.png

# ============================================
# Development Configuration
# ============================================
# Node environment
NODE_ENV=development

# Port for development server
PORT=3000

# ============================================
# Notes
# ============================================
# 1. Copy this file to .env.local: cp .env.example .env.local
# 2. Fill in all required values with your actual credentials
# 3. Never commit .env.local to version control
# 4. Keep your credentials secure and private
# 5. For production, use strong, unique values for all secrets
